pipeline {
    agent any
    stages {
        stage('Initialize Build') {
            steps {
                dir("${WORKSPACE}"){
                    sh '''
                        echo "BEGIN STAGE: Initialize"
                        ls
                        pwd
                        echo ${BUILD_NUMBER}
                        docker images
                        echo "END STAGE: Initialize"
                    '''
                }
                
            }
        }
    }
}
