pipeline {
    agent none
    stages {
        stage('Initialize Build') {
            agent {
                label 'docker-agent'
            }
            steps {
                dir("${WORKSPACE}"){
                    sh '''
                        echo "BEGIN STAGE: Initialize"
                        ls
                        pwd
                        echo ${BUILD_NUMBER}
                        docker images
                        echo "END STAGE: Initialize"
                    '''
                }
                
            }
        }
    }
}
